/**index.wxss**/
.container {
  padding: 0 0 200rpx 0;
  display: flex;
  flex-direction: column;
  background-color: #F2f2f2;
  min-height: 100%;
}

.head-image {
  width: 100%;
  height: 562.5rpx;
  top: 0px;
}

.content {
  margin-top: 15px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 14px;
  text-indent: 2em;
  font-family: Helvetica;
  line-height: 25px;
}

.title {
  text-align: center;
  font-size: 18px;
  font-weight: 500;
}

.text-content {
  margin-top: 10px;
  padding: 0 30rpx;
}